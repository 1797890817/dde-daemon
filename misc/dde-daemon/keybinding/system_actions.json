{
	"Actions": [
	{
		"Key": "launcher",
		"Action": "dde-launcher"
	},
	{
		"Key": "terminal",
		"Action": "/usr/lib/deepin-daemon/default-terminal"
	},
	{
		"Key": "lock-screen",
		"Action": "dde-lock"
	},
	{
		"Key": "show-dock",
		"Action": "dbus-send --type=method_call --dest=com.deepin.dde.dock /com/deepin/dde/dock com.deepin.dde.dock.ToggleShow"
	},
	{
		"Key": "logout",
		"Action": "dde-shutdown"
	},
	{
		"Key": "terminal-quake",
		"Action": "deepin-terminal --quake-mode"
	},
	{
		"Key": "screenshot",
		"Action": "deepin-screenshot"
	},
	{
		"Key": "screenshot-fullscreen",
		"Action": "deepin-screenshot -f"
	},
	{
		"Key": "screenshot-window",
		"Action": "deepin-screenshot -w"
	},
	{
		"Key": "screenshot-delayed",
		"Action": "deepin-screenshot -d 5"
	},
	{
		"Key": "file-manager",
		"Action": "nautilus"
	},
	{
		"Key": "disable-touchpad",
		"Action": "gsettings set com.deepin.dde.touchpad touchpad-enabled false"
	}
	]
}
